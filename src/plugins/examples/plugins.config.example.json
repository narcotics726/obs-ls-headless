{
  "plugins": [
    {
      "name": "echo-plugin",
      "command": "node",
      "args": ["src/plugins/examples/plugins/echo-plugin.js"],
      "enabled": true,
      "rpcTimeoutMs": 5000,
      "handshakeTimeoutMs": 3000,
      "shutdownTimeoutMs": 2000,
      "events": {
        "include": ["*"],
        "exclude": []
      },
      "allowReverseRpc": false,
      "routePrefix": "/api/plugins/echo"
    }
  ]
}
